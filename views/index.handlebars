<div class="card">
    <h1 class="z-depth-3 center-align">Read the Latest from The Verge in Tech</h1>
</div>

<div class="row">
  <div class="col s2"></div>
  <div class ="col s8 left-align">
    {{#each article}}
      <div class="card-panel">
        <h4>{{this.title}}</h4>
        <p>Read article <a href="{{this.link}}" target="_blank">here</a></p>
      </div>
    {{/each}}
  </div>
</div>